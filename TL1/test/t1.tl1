% ** SPRITE TEST PROGRAM **
PROC WAIT,TIME
%--- MAIN ---
VAR I
BEGIN
  WRITE(1:"Do ")
  FOR I:=1 TO 10 DO [
    WRITE(1:I,CRLF)
    TIME
    ]
  WAIT
END
%-- PROCEDURE WAIT --
WAIT
VAR I,J,K
BEGIN
  FOR I:=0 TO 1 DO [
    FOR J:=0 TO 255 DO [
      FOR K:=0 TO 255 DO []]]
END
%-- PROCEDURE TIME --
TIME
VAR I,J
BEGIN
  FOR I:=0 TO 10 DO [
    FOR J:=0 TO 150 DO []]
END
