% ** SPRITE TEST PROGRAM **
PROC WAIT
FUNC TIME
%--- MAIN ---
VAR MMI,MMJ,MMK
BEGIN
  MMI:=1 MMJ:=2 MMK:=3
  WAIT(4,5)
  WRITE(1:MMI,CRLF)
END
%-- PROCEDURE WAIT --
WAIT(WWA,WWB)
VAR WWJ,WWK
BEGIN
  WWJ:=4
  WWK:=TIME(6,7)
  WRITE(1:MMI,WWJ,WWA,WWB,WWK,CRLF)
END
%-- PROCEDURE TIME --
TIME(TTA,TTB)
VAR TTK,TTJ
BEGIN
  TTK:=5
  WRITE(1:MMI,TTK,TTA,TTB,CRLF)
  RETURN TTK
END
